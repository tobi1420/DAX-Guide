Currently Filtered = 

IF(
    ISFILTERED(
        Survey[2. Your Age]
    ),
    " | Age: " &
    CONCATENATEX(
        VALUES(Survey[2. Your Age]),
         Survey[2. Your Age], ", ")
)

& 

